#!/bin/bash
# This script contacts github and merges repo
# this script is run by an alais to luanch this 
# script before editing local files.

# this command changes the directory to the local repo directory
cd /home/htmlgifted/Github/configs &&

# this line lists current branch
git branch &&

# pull any changes you made online
git pull &&

# pull master changes if any
git fetch upstream &&

#merge with current branch
git merge upstream/ubuntu

echo "Successful Merge. Work on $USER"
